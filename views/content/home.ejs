<section class="container">
	<div id="mobile">
		<form method="GET" action="" id="getreq" hidden></form>
		<div class="content_box">
			<img class="logo" alt="stack_logo" src="/image/logo_with_text.png" />
			<% if(locals.categories) {%> <%categories.forEach(x=> {%>
			<a class="go_btn" endpoint="/content/category/<%= x.type %>"
				><%= x.type %></a
			>
			<% }) ; %><%}%> <% if(locals.sub_categories) {%>
			<%sub_categories.forEach(x=> {%>
			<a class="go_btn" endpoint="/content/category/<%= x.type %>"
				><%= x.name %></a
			>
			<% }) ; %><%}%>
		</div>
	</div>
</section>
